{
    "version": "1.7.0-dev.20150903",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "inlineSourceMap": true,
        "inlineSources": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "compileOnSave": false,
    "buildOnSave": false,
    "files": [
        "./src/Game/Tile.ts",
        "./src/Game/index.ts",
        "./test/somethingelse.ts",
        "./typings/lodash/lodash.d.ts",
        "./typings/mocha/mocha.d.ts",
        "./typings/react/react.d.ts",
        "./typings/should/should.d.ts",
        "./typings/tsd.d.ts",
        "./src/DigitalDisplay.tsx",
        "./src/Dude.tsx",
        "./src/Tile.tsx",
        "./src/Timer.tsx",
        "./src/index.tsx"
    ]
}
